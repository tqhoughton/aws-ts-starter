{
  "name": "aws-starter-ts",
  "version": "1.0.0",
  "description": "AWS starter template",
  "scripts": {
    "tsc": "tsc --noEmit",
    "deploy": "./deploy/deploy.sh",
    "yaml": "ts-node ./deploy/yaml.ts",
    "build": "webpack --config deploy/webpack.config.js --mode none",
    "test": "npx jest",
    "test:verbose": "ENABLE_LOGGING=1 npm run test",
    "destroy": "aws cloudformation delete-stack --stack-name my-stack-name",
    "list-buckets": "aws s3 ls"
  },
  "author": "Turner Houghton",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/aws-lambda": "^8.10.64",
    "@types/lodash": "^4.14.162",
    "awesome-typescript-loader": "^5.2.1",
    "aws-sdk": "^2.783.0",
    "babel-jest": "^26.6.1",
    "glob": "^7.1.6",
    "jest": "^26.6.1",
    "js-yaml": "^3.14.0",
    "ts-jest": "^26.4.2",
    "ts-loader": "^8.0.7",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0"
  }
}
